<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="id.sch.smktelkom_mlg.learn.looweapp.MainActivity">

    <ImageView
        android:id="@+id/imageViewSemur"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_weight="1"
        android:src="@mipmap/semur_jengkol" />

    <ImageView
        android:id="@+id/imageViewOncom"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="center_horizontal|top"
        android:layout_weight="1"
        android:src="@mipmap/oncom" />

    <ImageView
        android:id="@+id/imageViewBubur"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="right|top"
        android:layout_weight="1"
        android:src="@mipmap/bubur" />

    <ImageView
        android:id="@+id/imageViewRujak"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="left|center_vertical"
        android:layout_weight="1"
        android:src="@mipmap/rujak" />

    <ImageView
        android:id="@+id/imageViewGulai"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="right|center_vertical"
        android:layout_weight="1"
        android:src="@mipmap/gulai_kambing" />

    <ImageView
        android:id="@+id/imageViewAyamrica"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:src="@mipmap/ayam_rica" />

    <ImageView
        android:id="@+id/imageViewIkanbalado"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="left|bottom"
        android:layout_weight="1"
        android:src="@mipmap/ikan_balado" />

    <ImageView
        android:id="@+id/imageViewSate"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_weight="1"
        android:src="@mipmap/sate" />

    <ImageView
        android:id="@+id/imageViewSoto"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="right|bottom"
        android:layout_weight="1"
        android:src="@mipmap/soto" />

</FrameLayout>
